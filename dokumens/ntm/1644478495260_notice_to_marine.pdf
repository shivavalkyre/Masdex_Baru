</div>        `
        containerCurrentElement.appendChild(containerAbsolute)

        const xButton = containerAbsolute.querySelector('.x-button')
        xButton.addEventListener('click', removeTooltip)

        setTimeout(() => {
            xButton.removeEventListener('click', removeTooltip)
            xButton.parentElement.parentElement.remove()
        }, 3000);
    }

    function removeTooltip(e){
        e.stopPropagation();
        e.preventDefault();
        
        e.target.parentElement.parentElement.remove()
    }
    // end handle tooltip development
</script>


